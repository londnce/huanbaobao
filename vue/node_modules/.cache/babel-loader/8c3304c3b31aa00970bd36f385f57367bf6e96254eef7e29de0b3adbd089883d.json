ble-column prop=\"phone\" label=\"电话\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"email\" label=\"邮箱\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"operate\" label=\"操作\" width=\"300\" align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"success\">编辑<i class=\"el-icon-edit\"></i></el-button>\r\n                    <el-button type=\"danger\">删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<style>\r\n    .headerBg {\r\n        background-color: #cccccc;\r\n    }\r\n</style>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                tableData: [],\r\n                total: 0,\r\n                pageNum: 1,\r\n                pageSize: 5,\r\n            }\r\n        },\r\n        methods: {\r\n            handleSizeChange(pageSize) {\r\n                console.log(`每页 ${pageSize} 条`)\r\n                this.pageSize = pageSize\r\n                this.load()\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                console.log(`当前页: ${pageNum}`)\r\n                this.pageNum = pageNum\r\n                this.load()\r\n            }\r\n        },\r\n        created() {\r\n            //请求分页查询数据\r\n            this.load()\r\n        },\r\n        load() {\r\n            fetch(\"http://localhost:9090/admin/user/page?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize)\r\n                .then(res => res.json()).then(res=> {\r\n                console.log(res)\r\n                this.tableData = res.data\r\n                this.total = res.total\r\n            })\r\n        }\r\n\r\n    }\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}