{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"../utils/request\";\nexport default {\n  name: \"CategoryManage\",\n  data() {\n    return {\n      list: [],\n      total: 0,\n      listLoading: true,\n      pageNum: 1,\n      pageSize: 5,\n      parentId: 0\n    };\n  },\n  created() {\n    this.resetParentId();\n    this.getList();\n  },\n  watch: {\n    $route() {\n      this.resetParentId();\n      this.getList();\n    }\n  },\n  methods: {\n    resetParentId() {\n      this.pageNum = 1;\n      if (this.$route.query.parentId != null) {\n        this.parentId = this.$route.query.parentId;\n      } else {\n        this.parentId = 0;\n      }\n    },\n    handleAddProductCate() {\n      this.$router.push('/admin/addCate');\n    },\n    getList() {\n      this.listLoading = true;\n      request.post('/admin/category/page?', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          parentId: this.parentId\n        }\n      }).then(response => {\n        this.listLoading = false;\n        this.list = response.data.list;\n        this.total = response.data.total;\n      });\n    },\n    handleSizeChange(val) {\n      this.pageNum = 1;\n      this.pageSize = val;\n      this.getList();\n    },\n    handleCurrentChange(val) {\n      this.pageNum = val;\n      this.getList();\n    },\n    handleShowStatusChange(index, row) {\n      let ids = [];\n      ids.push(row.id);\n      data.append('ids', ids);\n      data.append('showStatus', row.showStatus);\n      request.post('/admin/statusChange/' + row.id).then(res => {\n        this.$message({\n          message: '修改成功',\n          type: 'success',\n          duration: 1000\n        });\n      });\n    },\n    handleShowNextLevel(index, row) {\n      this.$router.push({\n        path: '/admin/category',\n        query: {\n          parentId: row.id\n        }\n      });\n    },\n    handleUpdate(row) {\n      this.list = row;\n    },\n    handleDelete(index, row) {\n      this.$confirm('是否要删除该品牌', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        request.delete('/admin/category/delCate/' + row.id).then(res => {\n          this.$message({\n            message: '删除成功',\n            type: 'success',\n            duration: 1000\n          });\n          this.getList();\n        });\n      });\n    }\n  },\n  filters: {\n    levelFilter(value) {\n      if (value === 0) {\n        return '一级';\n      } else if (value === 1) {\n        return '二级';\n      }\n    },\n    disableNextLevel(value) {\n      if (value === 0) {\n        return false;\n      } else {\n        return true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AA2EA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;EACA;EACAC;IACAC;MACA;MACA;IACA;EACA;EACAC;IACAC;MACA;MACA;QACA;MACA;QACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MACAC;QACAC;UACAX;UACAC;UACAC;QACA;MACA,GACAU;QACA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;MACAC;MACApB;MACAA;MACAc;QACA;UACAO;UACAC;UACAC;QACA;MACA;IACA;IACAC;MACA;QAAAC;QAAAC;UAAApB;QAAA;MAAA;IACA;IACAqB;MACA;IAEA;IACAC;MACA;QACAC;QACAC;QACAR;MACA;QACAR;UACA;YACAO;YACAC;YACAC;UACA;UACA;QACA;MACA;IACA;EACA;EACAQ;IACAC;MACA;QACA;MACA;QACA;MACA;IACA;IACAC;MACA;QACA;MACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","list","total","listLoading","pageNum","pageSize","parentId","created","watch","$route","methods","resetParentId","handleAddProductCate","getList","request","params","then","handleSizeChange","handleCurrentChange","handleShowStatusChange","ids","message","type","duration","handleShowNextLevel","path","query","handleUpdate","handleDelete","confirmButtonText","cancelButtonText","filters","levelFilter","disableNextLevel"],"sourceRoot":"src/views","sources":["CategoryManage.vue"],"sourcesContent":["<template>\r\n    <div class=\"app-container\">\r\n        <el-card class=\"operate-container\" shadow=\"never\">\r\n            <i class=\"el-icon-tickets\" style=\"margin-top: 5px;\"></i>\r\n            <span style=\"margin-top: 5px; padding-left: 5px; padding-right: 1000px\">数据列表</span>\r\n            <el-button type=\"primary\" @click=\"handleAddProductCate()\" size=\"mini\">\r\n                <i class=\"el-icon-circle-plus-outline\">添加分类</i>\r\n            </el-button>\r\n        </el-card>\r\n        <div class=\"table-container\">\r\n            <el-table ref=\"productCateTable\"\r\n                      style=\"width: 100%\"\r\n                      :data=\"list\"\r\n                      v-loading=\"listLoading\" border>\r\n                <el-table-column label=\"编号\" width=\"100\" align=\"center\">\r\n                    <template slot-scope=\"scope\">{{scope.row.id}}</template>\r\n                </el-table-column>\r\n                <el-table-column label=\"分类名称\" align=\"center\">\r\n                    <template slot-scope=\"scope\">{{scope.row.name}}</template>\r\n                </el-table-column>\r\n                <el-table-column label=\"级别\" width=\"100\" align=\"center\">\r\n                    <template slot-scope=\"scope\">{{scope.row.level | levelFilter}}</template>\r\n                </el-table-column>\r\n                <el-table-column label=\"状态\" width=\"100\" align=\"center\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch\r\n                                @change=\"handleShowStatusChange(scope.$index, scope.row)\"\r\n                                :active-value=\"1\"\r\n                                :inactive-value=\"0\"\r\n                                v-model=\"scope.row.showStatus\">\r\n                        </el-switch>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"排序\" width=\"100\" align=\"center\">\r\n                    <template slot-scope=\"scope\">{{scope.row.sortOrder }}</template>\r\n                </el-table-column>\r\n                <el-table-column label=\"设置\" width=\"200\" align=\"center\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button\r\n                                size=\"mini\"\r\n                                :disabled=\"scope.row.level | disableNextLevel\"\r\n                                @click=\"handleShowNextLevel(scope.$index, scope.row)\">查看下级\r\n                        </el-button>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button\r\n                                size=\"mini\"\r\n                                @click=\"handleUpdate(scope.$index, scope.row)\">编辑\r\n                        </el-button>\r\n                        <el-button\r\n                                size=\"mini\"\r\n                                type=\"danger\"\r\n                                @click=\"handleDelete(scope.$index, scope.row)\">删除\r\n                        </el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n        </div>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    layout=\"total, sizes,prev, pager, next,jumper\"\r\n                    :page-size=\"pageSize\"\r\n                    :page-sizes=\"[5,10,15]\"\r\n                    :current-page=\"pageNum\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import request from \"../utils/request\";\r\n\r\n    export default {\r\n        name: \"CategoryManage\",\r\n        data() {\r\n            return {\r\n                list: [],\r\n                total: 0,\r\n                listLoading: true,\r\n                pageNum: 1,\r\n                pageSize: 5,\r\n                parentId: 0\r\n            }\r\n        },\r\n        created() {\r\n            this.resetParentId();\r\n            this.getList();\r\n        },\r\n        watch: {\r\n            $route() {\r\n                this.resetParentId();\r\n                this.getList();\r\n            }\r\n        },\r\n        methods: {\r\n            resetParentId(){\r\n                this.pageNum = 1;\r\n                if (this.$route.query.parentId != null) {\r\n                    this.parentId = this.$route.query.parentId;\r\n                } else {\r\n                    this.parentId = 0;\r\n                }\r\n            },\r\n            handleAddProductCate() {\r\n                this.$router.push('/admin/addCate');\r\n            },\r\n            getList() {\r\n                this.listLoading = true;\r\n                request.post('/admin/category/page?',{\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        parentId: this.parentId\r\n                    }\r\n                })\r\n                    .then(response => {\r\n                    this.listLoading = false;\r\n                    this.list = response.data.list;\r\n                    this.total = response.data.total;\r\n                });\r\n            },\r\n            handleSizeChange(val) {\r\n                this.pageNum = 1;\r\n                this.pageSize = val;\r\n                this.getList();\r\n            },\r\n            handleCurrentChange(val) {\r\n                this.pageNum = val;\r\n                this.getList();\r\n            },\r\n            handleShowStatusChange(index, row) {\r\n                let ids=[];\r\n                ids.push(row.id)\r\n                data.append('ids',ids);\r\n                data.append('showStatus',row.showStatus);\r\n                request.post('/admin/statusChange/'+row.id).then(res => {\r\n                    this.$message({\r\n                        message: '修改成功',\r\n                        type: 'success',\r\n                        duration: 1000\r\n                    });\r\n                });\r\n            },\r\n            handleShowNextLevel(index, row) {\r\n                this.$router.push({path: '/admin/category', query: {parentId: row.id}})\r\n            },\r\n            handleUpdate(row) {\r\n                this.list = row\r\n\r\n            },\r\n            handleDelete(index, row) {\r\n                this.$confirm('是否要删除该品牌', '提示', {\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                }).then(() => {\r\n                    request.delete('/admin/category/delCate/'+row.id).then(res => {\r\n                        this.$message({\r\n                            message: '删除成功',\r\n                            type: 'success',\r\n                            duration: 1000\r\n                        });\r\n                        this.getList();\r\n                    });\r\n                });\r\n            }\r\n        },\r\n        filters: {\r\n            levelFilter(value) {\r\n                if (value === 0) {\r\n                    return '一级';\r\n                } else if (value === 1) {\r\n                    return '二级';\r\n                }\r\n            },\r\n            disableNextLevel(value) {\r\n                if (value === 0) {\r\n                    return false;\r\n                } else {\r\n                    return true;\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}