{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"../utils/request\";\nexport default {\n  name: \"ProductManage\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 5,\n      dialogFormVisible: false,\n      form: {}\n    };\n  },\n  created() {\n    //请求分页查询数据\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/admin/product/page?\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          title: this.title\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.data;\n        this.total = res.data.total;\n      });\n    },\n    handleSizeChange(pageSize) {\n      console.log(`每页 ${pageSize} 条`);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(`当前页: ${pageNum}`);\n      this.pageNum = pageNum;\n      this.load();\n    },\n    edit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    cancel() {\n      this.dialogFormVisible = false;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/admin/product/delete/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    addPro() {\n      this.$router.push(\"/productManage/addPro\");\n    },\n    save() {\n      request.put(\"/admin/product/edit\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"编辑成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"编辑失败\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA0GA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;UACAR;UACAC;UACAQ;QACA;MACA,GACAC;QACAC;QACA;QACA;MACA;IACA;IACAC;MACAD;MACA;MACA;IACA;IACAE;MACAF;MACA;MACA;IACA;IACAG;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACAT;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAU;MACA;IACA;IACAC;MACAX;QACA;UACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","tableData","total","pageNum","pageSize","dialogFormVisible","form","created","methods","load","request","params","title","then","console","handleSizeChange","handleCurrentChange","edit","cancel","del","addPro","save"],"sourceRoot":"src/views","sources":["ProductManage.vue"],"sourcesContent":["<template>\r\n    <div class=\"product\">\r\n        <h1>商品管理</h1>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-input style=\"width: 200px\" suffix-icon=\"el-icon-search\" placeholder=\"请输入商品\" v-model=\"title\"></el-input>\r\n            <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n        </div>\r\n        <div style=\"margin: 10px 0\">\r\n            <el-button size=\"mini\" type=\"primary\" @click=\"addPro\">新增<i\r\n                    class=\"el-icon-circle-plus-outline\"></i>\r\n            </el-button>\r\n        </div>\r\n        <el-table :data=\"tableData\" border stripe header-cell-class-name=\"headerBg\" style=\"margin-top:15px\">\r\n            <el-table-column prop=\"id\" label=\"编号\" width=\"80\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"title\" label=\"商品名称\" width=\"150\" align=\"center\">\r\n            </el-table-column>\r\n<!--            <el-table-column prop=\"categoryOneName\" label=\"一级分类\" width=\"150\">-->\r\n<!--            </el-table-column>-->\r\n<!--            <el-table-column prop=\"categoryTwoName\" label=\"二级分类\" width=\"150\">-->\r\n<!--            </el-table-column>-->\r\n            <el-table-column prop=\"categoryThreeName\" label=\"分类名称\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"price\" label=\"商品价格\" width=\"70\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"num\" label=\"商品库存\" width=\"70\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"status\" label=\"商品状态\" width=\"50\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"createdTime\" label=\"创建时间\" width=\"150\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"modifiedTime\" label=\"修改时间\" width=\"150\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"modifiedUser\" label=\"修改人\" width=\"100\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\" align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    <div>\r\n                        <el-button size=\"mini\" type=\"success\" @click=\"edit(scope.row)\">编辑<i\r\n                                class=\"el-icon-edit\"></i>\r\n                        </el-button>\r\n                        <el-popconfirm\r\n                                style=\"margin-left: 5px\"\r\n                                confirm-button-text='确认'\r\n                                cancel-button-text='取消'\r\n                                icon=\"el-icon-info\"\r\n                                icon-color=\"red\"\r\n                                title=\"确定删除此商品？\"\r\n                                @confirm=\"del(scope.row.id)\">\r\n                            <el-button size=\"mini\" type=\"danger\" slot=\"reference\" @click=\"del(scope.row)\">删除<i\r\n                                    class=\"el-icon-remove-outline\"></i>\r\n                            </el-button>\r\n                        </el-popconfirm>\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n\r\n        <el-dialog title=\"商品信息\" :visible.sync=\"dialogFormVisible\" width=\"40%\" :close-on-click-modal=\"false\">\r\n            <el-form label-width=\"140px\" size=\"small\" style=\"width: 85%;\" :model=\"form\">\r\n                <el-form-item prop=\"title\" label=\"商品名称\">\r\n                    <el-input v-model=\"form.title\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item prop=\"price\" label=\"商品价格\">\r\n                    <el-input v-model=\"form.price\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item prop=\"image\" label=\"商品图片\">\r\n                    <el-upload action=\"/file/upload\" ref=\"image\" :on-success=\"handleImgUploadSuccess\">\r\n                        <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n                    </el-upload>\r\n                </el-form-item>\r\n                <el-form-item prop=\"itemType\" label=\"商品系列\">\r\n                    <el-input v-model=\"form.itemType\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item prop=\"num\" label=\"商品库存\">\r\n                    <el-input v-model=\"form.num\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item prop=\"status\" label=\"状态\">\r\n                    <el-input v-model=\"form.status\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"修改人\">\r\n                    <el-input v-model=\"form.adminName\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import request from \"../utils/request\";\r\n\r\n    export default {\r\n        name: \"ProductManage\",\r\n        data() {\r\n            return {\r\n                tableData: [],\r\n                total: 0,\r\n                pageNum: 1,\r\n                pageSize: 5,\r\n                dialogFormVisible: false,\r\n                form: {},\r\n            }\r\n        },\r\n        created() {\r\n            //请求分页查询数据\r\n            this.load()\r\n        },\r\n        methods: {\r\n            load() {\r\n                request.get(\"/admin/product/page?\", {\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        title: this.title\r\n                    }\r\n                })\r\n                    .then(res => {\r\n                        console.log(res)\r\n                        this.tableData = res.data.data\r\n                        this.total = res.data.total\r\n                    })\r\n            },\r\n            handleSizeChange(pageSize) {\r\n                console.log(`每页 ${pageSize} 条`)\r\n                this.pageSize = pageSize\r\n                this.load()\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                console.log(`当前页: ${pageNum}`)\r\n                this.pageNum = pageNum\r\n                this.load()\r\n            },\r\n            edit(row) {\r\n                this.form = row\r\n                this.dialogFormVisible = true\r\n            },\r\n            cancel() {\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n            },\r\n            del(id) {\r\n                request.delete(\"/admin/product/delete/\" + id).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"删除成功\")\r\n                        this.load()\r\n                    } else {\r\n                        this.$message.error(\"删除失败\")\r\n                    }\r\n                })\r\n            },\r\n            addPro(){\r\n                this.$router.push(\"/productManage/addPro\")\r\n            },\r\n            save(){\r\n                request.put(\"/admin/product/edit\", this.form).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"编辑成功\")\r\n                        this.dialogFormVisible = false\r\n                        this.load()\r\n                    } else {\r\n                        this.$message.error(\"编辑失败\")\r\n                    }\r\n                })\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .headerBg {\r\n        background-color: #cccccc;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}