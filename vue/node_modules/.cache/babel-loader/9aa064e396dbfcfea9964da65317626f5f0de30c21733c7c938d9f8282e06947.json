{"ast":null,"code":"import request from \"../utils/request\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 5,\n      username: \"\",\n      dialogFormVisible: false,\n      form: {}\n    };\n  },\n  created() {\n    //请求分页查询数据\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/admin/user/page?\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.data;\n        this.total = res.data.total;\n      });\n    },\n    handleSizeChange(pageSize) {\n      console.log(`每页 ${pageSize} 条`);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(`当前页: ${pageNum}`);\n      this.pageNum = pageNum;\n      this.load();\n    },\n    handleEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    edit() {\n      request.put(\"/admin/user/edit\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"编辑成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"编辑失败\");\n        }\n      });\n    },\n    cancel() {\n      this.dialogFormVisible = false;\n      this.load();\n    },\n    deleteUser(uid) {\n      request.delete(\"/admin/user/delete/\" + uid).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAwEA;AAEA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;UACAT;UACAC;UACAC;QACA;MACA,GACAQ;QACAC;QACA;QACA;MACA;IACA;IACAC;MACAD;MACA;MACA;IACA;IACAE;MACAF;MACA;MACA;IACA;IACAG;MACA;MACA;IACA;IACAC;MACAP;QACA;UACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAQ;MACA;MACA;IACA;IACAC;MACAT;QACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["data","tableData","total","pageNum","pageSize","username","dialogFormVisible","form","created","methods","load","request","params","then","console","handleSizeChange","handleCurrentChange","handleEdit","edit","cancel","deleteUser"],"sourceRoot":"src/views","sources":["UserManage.vue"],"sourcesContent":["<template>\r\n    <div class=\"user\">\r\n      <h1>用户管理</h1>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-input style=\"width: 200px\" suffix-icon=\"el-icon-search\" placeholder=\"请输入名称\" v-model=\"username\"></el-input>\r\n            <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n        </div>\r\n        <el-table :data=\"tableData\" border stripe header-cell-class-name=\"headerBg\" style=\"margin-top:50px\">\r\n            <el-table-column prop=\"uid\" label=\"uid\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"username\" label=\"用户名\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"gender\" label=\"性别\" width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"phone\" label=\"电话\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"email\" label=\"邮箱\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"operate\" label=\"操作\" width=\"300\" align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑<i class=\"el-icon-edit\"></i></el-button>\r\n                    <el-popconfirm\r\n                            confirm-button-text='确认'\r\n                            cancel-button-text='取消'\r\n                            icon=\"el-icon-info\"\r\n                            icon-color=\"red\"\r\n                            title=\"确定删除此用户？\"\r\n                            @contextmenu=\"\"\r\n                    >\r\n                        <el-button type=\"danger\" @click=\"deleteUser(scope.row.uid)\">删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n                    </el-popconfirm>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n        <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\"  width=\"400px\">\r\n            <el-form label-width=\"100px\">\r\n                <el-form-item label=\"电话\">\r\n                    <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"邮箱\">\r\n                    <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\">\r\n                    <el-input v-model=\"form.gender\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"cancel\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"edit\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<style>\r\n    .headerBg {\r\n        background-color: #cccccc;\r\n    }\r\n</style>\r\n\r\n<script>\r\nimport request from \"../utils/request\";\r\n\r\nexport default {\r\n        data() {\r\n            return {\r\n                tableData: [],\r\n                total: 0,\r\n                pageNum: 1,\r\n                pageSize: 5,\r\n                username: \"\",\r\n                dialogFormVisible: false,\r\n                form: {},\r\n            }\r\n        },\r\n        created() {\r\n            //请求分页查询数据\r\n            this.load()\r\n        },\r\n        methods: {\r\n            load(){\r\n                request.get(\"/admin/user/page?\" ,{\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        username: this.username\r\n                    }\r\n                })\r\n                    .then(res=>{\r\n                    console.log(res)\r\n                        this.tableData = res.data.data\r\n                        this.total = res.data.total\r\n                })\r\n            },\r\n            handleSizeChange(pageSize) {\r\n                console.log(`每页 ${pageSize} 条`)\r\n                this.pageSize = pageSize\r\n                this.load()\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                console.log(`当前页: ${pageNum}`)\r\n                this.pageNum = pageNum\r\n                this.load()\r\n            },\r\n            handleEdit(row){\r\n                this.form = row\r\n                this.dialogFormVisible = true\r\n            },\r\n            edit(){\r\n                request.put(\"/admin/user/edit\",this.form).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"编辑成功\")\r\n                        this.dialogFormVisible = false\r\n                        this.load()\r\n                    }else {\r\n                        this.$message.error(\"编辑失败\")\r\n                    }\r\n                })\r\n            },\r\n            cancel(){\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n            },\r\n            deleteUser(uid){\r\n                request.delete(\"/admin/user/delete/\"+uid).then(res=>{\r\n                    if (res){\r\n                        this.$message.success(\"删除成功\")\r\n                    }else {\r\n                        this.$message.error(\"删除失败\")\r\n                    }\r\n                })\r\n            }\r\n        },\r\n    }\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}