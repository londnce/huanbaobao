{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"../utils/request\";\nexport default {\n  name: \"ProductManage\",\n  data() {\n    return {\n      tableData: '',\n      pageSize: 10,\n      total: 100,\n      currentPage: 1\n    };\n  },\n  methods: {\n    page(currentPage) {\n      const _this = this;\n      request.get('/admin/product/list/' + currentPage + '/' + this.pageSize).then(function (resp) {\n        _this.tableData = resp.data.data;\n        _this.total = resp.data.total;\n      });\n    },\n    edit(rows) {\n      this.$router.push('/update?id=' + rows.id);\n    },\n    del(rows) {\n      const _this = this;\n      this.$confirm('确认删除【' + rows.name + '】吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        request.delete('/admin/product/delete/' + rows.id).then(function (resp) {\n          if (resp.data) {\n            _this.$alert('【' + rows.name + '】已删除', '', {\n              confirmButtonText: '确定',\n              callback: action => {\n                location.reload();\n              }\n            });\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    }\n  },\n  created() {\n    const _this = this;\n    request.get('/admin/product/list/1/' + this.pageSize).then(function (resp) {\n      _this.tableData = resp.data.data;\n      _this.total = resp.data.total;\n    });\n  }\n};","map":{"version":3,"mappings":";AA4EA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACAC;QACAC;QACAA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MACA;QACAC;QACAC;QACAC;MACA;QACAN;UACA;YACAC;cACAG;cACAG;gBACAC;cACA;YACA;UACA;QACA;MACA;QACA;UACAF;UACAG;QACA;MACA;IACA;EACA;EACAC;IACA;IACAV;MACAC;MACAA;IACA;EACA;AACA","names":["name","data","tableData","pageSize","total","currentPage","methods","page","request","_this","edit","del","con