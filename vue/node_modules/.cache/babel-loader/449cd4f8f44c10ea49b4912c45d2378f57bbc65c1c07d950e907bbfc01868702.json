{"ast":null,"code":"import request from \"../utils/request\";\nexport default {\n  name: \"ProductManage\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 5,\n      dialogFormVisible: false,\n      form: {}\n    };\n  },\n  created() {\n    //请求分页查询数据\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/admin/product/page?\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.data;\n        this.total = res.data.total;\n      });\n    },\n    handleSizeChange(pageSize) {\n      console.log(`每页 ${pageSize} 条`);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(`当前页: ${pageNum}`);\n      this.pageNum = pageNum;\n      this.load();\n    },\n    edit() {\n      request.put(\"/admin/product/edit\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"编辑成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"编辑失败\");\n        }\n      });\n    },\n    cancel() {\n      this.dialogFormVisible = false;\n      this.load();\n    },\n    del(pid) {\n      request.delete(\"/admin/product/delete/\" + pid).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA6CA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;UACAR;UACAC;UACAQ;QACA;MACA,GACAC;QACAC;QACA;QACA;MACA;IACA;IACAC;MACAD;MACA;MACA;IACA;IACAE;MACAF;MACA;MACA;IACA;IACAG;MACAP;QACA;UACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAQ;MACA;MACA;IACA;IACAC;MACAT;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","tableData","total","pageNum","pageSize","dialogFormVisible","form","created","methods","load","request","params","username","then","console","handleSizeChange","handleCurrentChange","edit","cancel","del"],"sourceRoot":"src/views","sources":["ProductManage.vue"],"sourcesContent":["<template>\r\n    <div style=\"margin-top: 60px;margin-left:80px;border: 0 solid red;\">\r\n        <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\r\n            <el-table-column prop=\"id\" label=\"编号\" width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"name\" label=\"商品名称\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"description\" label=\"商品描述\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"price\" label=\"商品价格\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"stock\" label=\"商品库存\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"categoryOneName\" label=\"商品分类\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"categoryTwoName\" label=\"创建时间\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"categoryTwoName\" label=\"修改时间\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"categoryThreeName\" label=\"修改人\" width=\"150\">\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button size=\"mini\" type=\"success\" @click=\"edit(scope.row)\">编辑\r\n                    </el-button>\r\n                    <el-button size=\"mini\" type=\"danger\" @click=\"del(scope.row)\">删除\r\n                    </el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import request from \"../utils/request\";\r\n\r\n    export default {\r\n        name: \"ProductManage\",\r\n        data() {\r\n            return {\r\n                tableData: [],\r\n                total: 0,\r\n                pageNum: 1,\r\n                pageSize: 5,\r\n                dialogFormVisible: false,\r\n                form: {},\r\n            }\r\n        },\r\n        created() {\r\n            //请求分页查询数据\r\n            this.load()\r\n        },\r\n        methods: {\r\n            load() {\r\n                request.get(\"/admin/product/page?\", {\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        username: this.username\r\n                    }\r\n                })\r\n                    .then(res => {\r\n                        console.log(res)\r\n                        this.tableData = res.data.data\r\n                        this.total = res.data.total\r\n                    })\r\n            },\r\n            handleSizeChange(pageSize) {\r\n                console.log(`每页 ${pageSize} 条`)\r\n                this.pageSize = pageSize\r\n                this.load()\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                console.log(`当前页: ${pageNum}`)\r\n                this.pageNum = pageNum\r\n                this.load()\r\n            },\r\n            edit() {\r\n                request.put(\"/admin/product/edit\", this.form).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"编辑成功\")\r\n                        this.dialogFormVisible = false\r\n                        this.load()\r\n                    } else {\r\n                        this.$message.error(\"编辑失败\")\r\n                    }\r\n                })\r\n            },\r\n            cancel() {\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n            },\r\n            del(pid) {\r\n                request.delete(\"/admin/product/delete/\" + pid).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"删除成功\")\r\n                        this.load()\r\n                    } else {\r\n                        this.$message.error(\"删除失败\")\r\n                    }\r\n                })\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}